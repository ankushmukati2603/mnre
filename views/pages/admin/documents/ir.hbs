<div class="_wrapper _admin_wrapper">
    <div class="_section_wrap _energy_list">
        <div class="_table_listings">
            <div class="_top">
                <p class="_head">{{heading}}</p>
                <a class="button" href="javascript:void(0)" onclick="_irModal('{{form}}Modal','{{form}}')">Add New
                    Listing</a>
            </div>

            <table id="table_id" class="display">
                <thead>
                    <tr>
                        <th>Title</th>
                        <th>Description</th>
                        <th>Meta Keywords</th>
                        <th>Country</th>
                        <th>Doctype</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each data}}
                    <tr>
                        <td>{{this.title}}</td>
                        <td>{{this.description}}</td>
                        <td>{{this.meta_keywords}}</td>
                        <td>{{this.country}}</td>
                        <td>{{this.doctype}}</td>
                        <td>
                            <button onclick="_getIrById('{{this._id}}','irModal')">Edit</button>
                            <button onclick="_removeIrById('{{this._id}}')">Delete</button>
                        </td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>


