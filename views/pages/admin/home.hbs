<section class="section is-small">
    <div class="container">
        <div class="columns is-multiline">
            {{#each editable_pages}}
            <div class="column is-4">
                <div class="card admin_edit">
                    <div class="card-header">
                        <div class="card-header-title">
                            {{this.title}}
                        </div>
                    </div>
                    <div class="card-content admin_edit_content">
                        {{#if editable_sections}}
                        <ul class="editPage_section_list">
                            {{#each this.editable_sections}}
                            <li class="editPage_section_list-item">
                                <div class="is-pulled-left">
                                    {{this.title}}
                                </div>
                                {{#if this.class}}
                                <div class="is-pulled-right">
                                    <span class="{{this.class}} edit_button_text">
                                        Edit
                                    </span>
                                </div>
                                {{else}}
                                <div class="is-pulled-right">
                                    <span class="edit_button_text">
                                        Edit
                                    </span>
                                </div>
                                {{/if}}
                            </li>
                            {{/each}}
                        </ul>
                        {{/if}}
                    </div>
                </div>
            </div>
            {{/each}}

        </div>
    </div>
</section>
