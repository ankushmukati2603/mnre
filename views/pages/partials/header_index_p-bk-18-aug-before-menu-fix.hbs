	/* media queries */
	/* ------------------------------------------------------------------------- */

	/* mobile small (iphone 3/4/5) ----------- */
	@media only screen and (max-width : 320px) {
		.grid-layout {
			grid-template-columns: repeat(auto-fill, minmax(66px, 1fr));
			grid-gap: 10px;
			grid-auto-rows: minmax(85px, auto);
			grid-auto-flow: dense;
			padding: 10px;
			display: grid;
			justify-items: center;
			width: 100%;
		}

		.grid-item {
			font-size: 0.6em
		}

		.modal-content {
			padding-top: 0px;
			height: 100%;
		}
	}

	/* mobile large (android) ----------- */
	@media only screen and (max-width : 685px) {
		.searchd {
			display: none;
		}

		.grid-layout {
			grid-template-columns: repeat(auto-fill, minmax(97px, 1fr));
			grid-gap: 0px;
			grid-auto-rows: minmax(90px, auto);
			grid-auto-flow: dense;
			padding: 0px;
			display: grid;
			justify-items: center;
			width: 100%;
		}

		.grid-item.sub-sub-grid {
			grid-column-end: span 3;
			grid-row-end: span 1;
		}

		.span-3 .service-content2 {
			top: 29%;
		}

		.span-3 .service-content2 img {
			width: 25%;
		}

		.service-content2 img {
			width: 40%;
		}

		.span-3 .front-content2 {
			font-size: x-large;
		}

		.front-content2 {
			font-size: 11px;
			line-height: 0px;
		}

		.sub-grid {
			font-size: 15px !important;
		}

		.service-content2 {
			top: 14%;
		}

		.grid-item {
			font-size: 0.6em
		}

		.modal-content {
			padding-top: 0px;
			height: 100%
		}

		.navbar {
			padding: 20px 10px;
		}

		.navbar-form {
			padding-top: 10px;
			padding-bottom: 10px;
		}
	}

	/* ipad portrait/landscape ----------- */
	@media only screen and (min-device-width : 768px) and (max-device-width : 1024px) {
		.modal-content {
			padding-top: 0px
		}

		.searchd {
			display: none;
		}
	}

	/* ipad portrait ----------- */
	@media only screen and (min-device-width : 768px) and (max-device-width : 1024px) and (orientation : portrait) {
		.modal-content {
			padding-top: 0px
		}

		.searchd {
			display: none;
		}
	}

	/* ipad landscape ----------- */
	@media only screen and (min-device-width : 768px) and (max-device-width : 1024px) and (orientation : landscape) {
		.modal-content {
			padding-top: 0px
		}

		.searchd {
			display: none;
		}
	}

	/* small displays ----------- */
	@media only screen and (max-width : 800px) {
		.searchd {
			display: none;
		}

		.mobile_none {
			display: none;
		}

		.mobile_handling {
			display: block
		}

		.solar_hrd,
		.hydro_hrd,
		.waste_hrd,
		.bio_hrd,
		.other_hrd,
		.pi_reports {
			grid-column-end: span 2 !important;
			grid-row-end: span 1 !important;
		}

		.solar_emerging,
		.hydro_emerg,
		.waste_emerging_areas,
		.bio_emerging_areas,
		.other_emerging_areas,
		.geo_thermal_energy,
		.biofuel_energy,
		.solar_sup_ins,
		.hydro_sup_ins,
		.waste_sup_ins,
		.bio_sup_ins,
		.other_sup_ins,
		.media_latest,
		.media_clippings,
		.media_ministry,
		.media_events,
		.media_advertisements,
		.what_isa,
		.re_about,
		.pi_circulars,
		.pi_notifications,
		.mobile-2 {
			grid-column-end: span 3 !important;
			grid-row-end: span 1 !important;
		}

		.ocean_energy,
		.seci_psu,
		.ireda_psu,
		.tender_recent,
		.tender_archived {
			grid-column-end: span 3 !important;
			grid-row-end: span 2 !important;
		}

		.mobile_modal {
			position: relative;
			/*top: -42px;*/
			text-align: center;
		}

		.mobile_handling_1 {
			display: none;
		}
	}

	@media only screen and (min-width : 1140px) and (max-width: 1223px) {
		.grid-layout {
			grid-template-columns: repeat(auto-fill, minmax(140px, 1fr));
			grid-gap: 10px;
			grid-auto-rows: 130px;
			grid-auto-flow: dense;
			padding: 10px;
			display: grid;
			justify-items: center;
			width: 100%;
		}
