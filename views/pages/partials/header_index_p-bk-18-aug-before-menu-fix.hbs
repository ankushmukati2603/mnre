		if (choice == true) {
			window.open("https://rti.gov.in", "_blank")
		} else {
			console.log('Redirect Cancelled')
		}
	});

	var subscribeForm = document.forms[1]
	$('.subbt').click(function(e){
		e.preventDefault()
		var items = $('[name="contentCheck"]:checked');
		var email = $('#subscribeEmail').val()
		var checkedValues = []
		for(var i = 0; i < items.length; i ++){
			checkedValues.push(items[i].value)
		}
		if(checkedValues.length > 0 && email){
			console.log(checkedValues,email)
			axios.post('/subscription/add',{
				subscriptions: checkedValues,
				email : email
			}).then((response)=>{
				return response.data
			}).catch((error)=>{
				console.log(error)
			}).then((data)=>{
				if(data.status == "Success"){
					$('.fancybox-close-small').click()
					$('#subscribeEmail').val('')
					for (var i = 0; i < items.length; i++) {
						items[i].checked = false
					}
					alert('Subscription Added')
				}
			})
		} else if(checkedValues.length == 0) {
			alert("Please Choose a Subscription")
		} else if(!email){
			alert("Please Enter an email")
		}
	})

