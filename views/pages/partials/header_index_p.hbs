    .grid-layout {
        grid-template-columns: 1fr 1fr 1fr;
        grid-gap: 0px;
        grid-auto-rows: 145px;
        grid-auto-flow: dense;
        padding: 10px;
        display: grid;
        justify-items: center;
        width: 100%;
