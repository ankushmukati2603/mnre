        /* Internet Explorer */
        -o-animation: fadein 0.5s;
        /* Opera < 12.1 */
        animation: fadein ease-in 0.5s;
        font-size: 2em
    }

    @keyframes fadein {
        from {
            opacity: 0.5;
            width: 50%;
            height: 50%;
        }

        to {
            opacity: 1;
            width: 100%;
            height: 100%
        }
    }

    /* Firefox < 16 */
    @-moz-keyframes fadein {
        from {
            opacity: 0;
            width: 50%;
            height: 50%;
        }

        to {
            opacity: 1;
            width: 100%;
            height: 100%
        }
    }

    /* Safari, Chrome and Opera > 12.1 */
    @-webkit-keyframes fadein {
        from {
            opacity: 0;
            width: 50%;
            height: 50%;
        }

        to {
            opacity: 1;
            width: 100%;
            height: 100%
        }
    }

