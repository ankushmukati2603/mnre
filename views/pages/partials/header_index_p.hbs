    });

    document.querySelector('#desktop_menu--trigger').addEventListener('click', function () {
        var menuText = document.querySelector('#desktop_menu--trigger span').textContent;
        if (menuText == 'Menu') {
            document.querySelector('#desktop_menu--trigger span').textContent = 'Close';
            document.querySelector('#desktop_menu--trigger .fa-bars').style.display = 'none';
            document.querySelector('#desktop_menu--trigger .fa-times-circle').style.display = 'inline-block';
        } else if (menuText == 'Close') {
            document.querySelector('#desktop_menu--trigger span').textContent = 'Menu';
            document.querySelector('#desktop_menu--trigger .fa-bars').style.display = 'inline-block';
            document.querySelector('#desktop_menu--trigger .fa-times-circle').style.display = 'none';
        }
    });

    var mobileNav = document.querySelector('.navbar-toggler');
    mobileNav.onclick = function () {
        mobileNav.classList.toggle('active');
    }

    $(document).keyup(function (e) {
        if (e.key === "Escape" || e.key == 27) {
            document.querySelector('#desktop_menu--trigger span').textContent = 'Menu';
            document.querySelector('#desktop_menu--trigger .fa-bars').style.display = 'inline-block';
            document.querySelector('#desktop_menu--trigger .fa-times-circle').style.display = 'none';
        }
    });

    var hideBackButon = function () {
        $('._back_btn').hide();
    }

    $('#contact_us').click(function () {
        hideBackButon();
        location.href = "/contact-mnre";
    });
    $('#re_invest').click(function () {
        hideBackButon();
        location.href = "/re-invest/2020";
    });
    $('#green_energy_corridor').click(function () {
        hideBackButon();
        location.href = "/green-energy-corridor/";
    });
    $('#human_resource_development').click(function () {
        hideBackButon();
        location.href = "/human-resource-development/";
    });
    $('#quality_standard_policy').click(function () {
        hideBackButon();
        location.href = "/quality-standard-policy/";
    });
    $('#international_solar_alliance').click(function () {
        hideBackButon();
        location.href = "/isa/";
    });
    $('#international_relations').click(function () {
        location.href = '/international-relations';
    });
    $('#parliament_questions').click(function () {
        location.href = "/parliament-questions";
    });
    $('#schedule_an_appointment').click(function () {
        //location.href = '/schedule-an-appointment'
        var choice = confirm("You are Redirecting to an External Site. Do you want to continue ?")
        if (choice == true) {
            window.open("https://evisitors.nic.in/", "_blank")
        } else {
            console.log('Redirect Cancelled')
        }
    });
    $('#rti').click(function () {
        location.href = "/right-to-information";
    });

    var subscribeForm = document.forms[1]
    $('.subbt').click(function (e) {
        e.preventDefault()
        var items = $('[name="contentCheck"]:checked');
        var email = $('#subscribeEmail').val()
        var checkedValues = []
        for (var i = 0; i < items.length; i++) {
            checkedValues.push(items[i].value)
        }
        if (checkedValues.length > 0 && email) {
            console.log(checkedValues, email)
            axios.post('/subscription/add', {
                subscriptions: checkedValues,
                email: email
            }).then((response) => {
                return response.data
            }).catch((error) => {
                console.log(error)
            }).then((data) => {
                if (data.status == "Success") {
                    $('.fancybox-close-small').click()
                    $('#subscribeEmail').val('')
                    for (var i = 0; i < items.length; i++) {
                        items[i].checked = false
                    }
                    alert('Subscription Added')
                }
            })
        } else if (checkedValues.length == 0) {
            alert("Please Choose a Subscription")
        } else if (!email) {
            alert("Please Enter an email")
        }
    })

</script>

