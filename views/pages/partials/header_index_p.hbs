            onSearchItemWiseResult: function (model, items, index) {
                var id = 'search_' + (model.name.toLowerCase());
                $tabs = $("#global-search-results-tabs");
                var div = '<div class="tab-pane fade show" id="' + id + '" role="tabpanel" aria-labelledby="' + id + '-tab"></div>'
                $tab = $(div);

                if (index === 0) $tab.addClass("active");

                var content = "<div class='row'>";
                for (var i = 0; i < items.length; i++) {
                    var item = items[i];
                    var link = model.route;

                    var date = item.createdAt; // toLocaleString({locale:'en-IN', timeZone: 'Asia/Kolkata'});

                    var route_vars = model.route_vars;
                    for (var key in route_vars) {
                        link = link.replace(key, item[route_vars[key]]);
                    }


                    var read_more_url = item.url;
                    if (item[model.file] != undefined) {
                        read_more_url = `img/documents${item[model.file]}`
                    }

                    content += '<div class="col-xs-12 col-sm-6 col-md-4 mb-4">';
                    content += '		<div class="card h-100">';
                    if (model.img !== undefined && model.img != '')
                        content += '			<img class="card-img-top" src="' + (item[model.img_key]) + '" alt="">';
                    content += '			<div class="card-body text-right">';
                    content += '				<p class="date fs-12">' + (new Date(date).toDateString()) + '</p>';
                    content += '				<h6 class="card-title text-left ellipsis mt-2">' + item[model.title_key] + '</h6>';
                    content += '				<p class="card-text text-left ellipsis mt-2">' + item[model.desc_key] + '</p>';
                    if (link != "/public-information/whos-who")
                        content += '				<a class="text-danger mt-3" target="_blank" href="' + read_more_url + '"><strong>READ MORE</strong></a>';
                    content += '			</div>';
                    content += '		</div>';
                    content += '</div>';

                }
                content += "</div>";
                $tab.append(content);
                $tabs.append($tab);

                $nav = $("#global-search-results-nav");
                var div = '<a class="nav-link" id="' + id + '-tab" data-toggle="pill" href="#' + id + '" role="tab" aria-controls="' + id + '" aria-selected="true"></a>';
                $li = $(div);
                $li.append('<h6 class="line-height-1">' + (model.title.toUpperCase()) + '</h6>');
                $li.append('<p class="ellipsis">Total ' + (items.length) + ' pages have matching content.</p>');
                if (index === 0) $li.addClass("active");
                $nav.append($li);
                return { tab: $tab.html(), nav: $li.html() };
            },
            onSearchComplete: function (results) {
                $(this).removeClass("loading");
                if (results.length === 0) {
